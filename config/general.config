<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
    <defaultroutes>1</defaultroutes>
    <smarturls>false</smarturls>
    <admin>
        <name>Séverine Ferron</name>
        <email>severine.ferron@bdsp.tm.fr</email>
    </admin>
    <theme>ascodocpsy</theme>
    <layout>asco.html</layout>
    <title>Ascodocpsy, réseau documentaire en santé mentale - Base SantéPsy</title>
    <css>
        <item>asco.css</item>
    </css>
    <security>
        <handler>SpipSecurity</handler>
    </security>
    <sessions>
        <use>false</use>
        <lifetime>0</lifetime>
    </sessions>
    <alias>
        <autosize.js>/js/autosize.js</autosize.js>
        <controls.js>/js/controls.js</controls.js>
        <load.js>/js/load.js</load.js>

        <formulaire.css>/css/ascodocpsy/formulaire.css</formulaire.css>
        <asco.css>/css/ascodocpsy/asco.css</asco.css>
        <homepage.css>/css/homepage.css</homepage.css>
    </alias>
</config>
